Ejercicio 2

#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int a,i;
    
    printf("Escriba un número:");
    scanf("%d",&a);
    
    for(i=1; i<=a; i++){
        printf("%d",i);
    }
    return 0;
}

/*
 Parámetros de tipo:
 %d --> número int
 %f --> número float
 %s --> cadena
 */
