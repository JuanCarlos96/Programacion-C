Ejercicio 6 (inacabado)
        
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int a,b,c;
    
    do{
        printf("Introduzca dos números enteros:\n");
        scanf("%d",&a);
        scanf("%d",&b);
        scanf("%d",&c);
    }while(a==b || a==c || b==c);
    
    if (a>b && b>c){
            printf("%d>%d>%d"a,b,c);
    }else{
        if(a>c && c>b){
            printf("%d>%d>%d"a,c,b);
        }else{
            
        }
    }
    
    return 0;
}
