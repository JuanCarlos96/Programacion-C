Ejercicio 5
        
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int a,b;
    
    do{
        printf("Introduzca dos números enteros: \n");
        scanf("%d",&a);
        scanf("%d",&b);
    }while(a==b);
    
    if (a==b){
        printf("Los números son iguales");
    }else{
        if (a>b){
            printf("%d es mayor que %d",a,b);
        }else{
            printf("%d es mayor que %d",b,a);
        }
    }
    
    return 0;
}
