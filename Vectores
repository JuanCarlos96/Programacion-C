Ejercicio 4

#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char** argv) {
    
    int aleatorio,vector[20],i,cont;
    
    srand(time(NULL));
    
    for(i=0; i<20; i++) {
        vector[i]=(rand() % 21)-10;
    }
    
    for(i=0; i<20; i++){
        if(vector[i]>0){
            cont++;
            printf("%d ",vector[i]);
        }
    }
    
    printf("\nSe han generado %d números positivos",cont);

    return 0;
}
