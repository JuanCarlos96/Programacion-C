Ejercicio 4

#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    printf("Introduzca un número: ");
    int n,i,suma,x;
    scanf("%d",&n);
    suma=0;
    
    for(i=1;i<=n;i++){
        suma=suma+i;
    }
    printf("%d",suma);
    
    return 0;
}
