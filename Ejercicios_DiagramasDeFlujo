Ejercicio 8
        
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int tam,par,impar;
    
    printf("Introduzca el número de elementos:\n");
    scanf("%d",&tam);
    
    for(int i=1;i<=tam;i++){
        if(i%2==0){
            par+=i;
        }else{
            impar+=i;
        }
    }
    
    printf("La suma de los pares es %d",par);
    printf("\nLa suma de los impares es %d",impar);
    
    return 0;
}
--------------------------------------------------------------------------------
Ejercicio 9
        
#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

int main(int argc, char** argv) {
    
    int num,tam,min=INT_MAX,max=INT_MIN;
    
    printf("Introduce el número de elementos:");
    scanf("%d",&tam);
    
    for(int i=1;i<=tam;i++){
        printf("Introduce un número:");
        scanf("%d",&num);
        
        if(num<min){
            min=num;
        }
        if(num>max){
            max=num;
        }
    }
    
    printf("El mayor es %d",max);
    printf("\nEl mínimo es %d",min);
    
    return 0;
}
