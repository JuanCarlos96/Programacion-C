Ejercicio 1

#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv) {
    
    int a;
    
    printf("Introduzca un número: ");
    
    scanf("%d",&a);
    
    if (a==0){
        printf("Es cero");    
    }
    else {
        if (a>0){
            printf("Es positivo");
        }
        else{
            printf("Es negativo");
        }
    }
    
    return 0;
}
