Ejercicio 3 (while)

#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char** argv) {
    
    int n,aleatorio;
    
    srand(time(NULL));
    aleatorio = rand() % 11;
    
    printf("Adivine un número entre entre 0 y 10: ");
    scanf("%d",&n);
    
    while(n!=aleatorio){
        printf("\nNo ha acertado.\nAdivine un número entre 0 y 10: ");
        scanf("%d",&n);
    }      
    
    printf("Has acertado");
    
    return 0;
}
--------------------------------------------------------------------------------
Ejercicio 3 (for)

#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char** argv) {
    
    int n,aleatorio;
    
    srand(time(NULL));
    aleatorio = rand() % 11;
    
    printf("Adivine un número entre entre 0 y 10: ");
    scanf("%d",&n);
    
    for(;n!=aleatorio;){
        printf("\nNo ha acertado.\nAdivine un número entre 0 y 10: ");
        scanf("%d",&n);
    }      
    
    printf("Has acertado");
    
    return 0;
}
--------------------------------------------------------------------------------
Ejercicio 3 (do while)

#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(int argc, char** argv) {
    
    int n,aleatorio;
    
    srand(time(NULL));
    aleatorio = rand() % 11;
    
    do{
        printf("\nAdivine un número entre 0 y 10: ");
        scanf("%d",&n);
    } while(n!=aleatorio);  
    
    printf("Has acertado");
    
    return 0;
}
